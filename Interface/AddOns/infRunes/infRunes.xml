<Ui xmlns="http://www.blizzard.com/wow/ui/"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI-Wrath.xsd">
  <Script file="infRunes.lua"/>

  <Frame name="RuneButtonIndividualTemplateC" virtual="true" hidden="false">
    <Size>
      <AbsDimension x="18" y="18"/>
    </Size>
    <Frames>
		<Frame name="$parentBorder">
			<Size x="29" y="29"/>
			<Anchors>
				<Anchor point="CENTER">
					<Offset x="0" y="0"/>
				</Anchor>
			</Anchors>
			<Scripts>
				<OnLoad>
					self:SetFrameLevel(self:GetFrameLevel()+1);
				</OnLoad>
			</Scripts>
		</Frame>
    </Frames>
    <Layers>
		
		<Layer level="ARTWORK">
			<Texture name="$parentRune" file="Interface\CharacterFrame\DeathKnight-BloodRune">
				<Size>
					<AbsDimension x="25" y="25"/>
				</Size>
				<Anchors>
					<Anchor point="CENTER">
						<Offset>
							<AbsDimension x="0" y="0"/>
						</Offset>
					</Anchor>
				</Anchors>
			</Texture>
		</Layer>
    </Layers>
    <Scripts>
      <OnLoad>
        RuneButtonC_OnLoad(self);
      </OnLoad>
    </Scripts>
  </Frame>

  <Frame name="RuneFrameC" parent="UIParent" frameStrata="LOW" toplevel="true" movable="true" hidden="false">    
    <Size>
      <AbsDimension x="56" y="110"/>
    </Size>
    <Anchors>
      <Anchor point="CENTER" relativeTo="UIParent" relativePoint="BOTTOMLEFT">
        <Offset>
          <AbsDimension x="500" y="300"/>
        </Offset>
      </Anchor>
    </Anchors>
    <Scripts>
      <OnLoad>
        RuneFrameC_OnLoad(self);
      </OnLoad>
    </Scripts>
  </Frame>
	
  <Frame name="RuneFrameBack" parent="RuneFrameC" frameStrata="LOW" toplevel="true" movable="true" hidden="false">    
    <Size>
      <AbsDimension x="55" y="112"/>
    </Size>
    <Anchors>
      <Anchor point="TOPLEFT" relativeTo="RuneFrameC" relativePoint="TOPLEFT">
        <Offset>
          <AbsDimension x="3" y="-3"/>
        </Offset>
      </Anchor>
    </Anchors>
		<Backdrop bgFile="Interface\AddOns\infRunes\textures\back.tga" tile="false">
      <BackgroundInsets left="0" right="0" top="0" bottom="0"/>
    </Backdrop>
  </Frame>
  
  <Frame name="RuneButtonIndividual1C" inherits="RuneButtonIndividualTemplateC" parent="RuneFrameC" id="1" frameStrata="MEDIUM">
    <Anchors>
      <Anchor point="TOPLEFT" relativeTo="RuneFrameC" relativePoint="TOPLEFT">
        <Offset>
          <AbsDimension x="0" y="0"/>
        </Offset>
      </Anchor>
    </Anchors>
  </Frame>

  <Frame name="RuneButtonIndividual2C" inherits="RuneButtonIndividualTemplateC" parent="RuneFrameC" id="2" frameStrata="MEDIUM">
